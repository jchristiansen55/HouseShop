<form ref='uploadForm' id='uploadForm' encType="multipart/form-data"  action="<% if (locals.fa17g09_env_prefix) { %>/<%= fa17g09_env_prefix %><%}%>/listings" method="post">
  <fieldset>
    <legend>Create a new listing</legend>
    <div class="listingForm-size">
        <label>
          Address:
          <input type="string" name="Address" class="form-control input-sm" placeholder="Address">
          <br>

          City:
          <input type="string" name="city" class="form-control input-sm" placeholder="City">
          <br>
          
          State:
          <input type="string" name="state" class="form-control input-sm" placeholder="State">
          <br>
          
          ZipCode:
          <input type="string" name="zipcode" class="form-control input-sm" placeholder="Zip Code">
          <br>

          Number of Bedrooms:
          <input type="string" name="numBedrooms" class="form-control input-sm" placeholder="Number of Bedrooms">
          <br>
          
          Number of Bathrooms:
          <input type="string" name="numBathrooms" class="form-control input-sm" placeholder="Number of Bathrooms">
          <br>
          
          Square Footage:
          <input type="string" name="squareFeet" class="form-control input-sm" placeholder="Square Footage">
          <br>
          
          Price:
          <input type="string" name="price" class="form-control input-sm" placeholder="Price">
          <br>
          
          Description:
          <input type="string" name="description" class="form-control input-sm" placeholder="Description">
          <br>

          <br> Choose a primary picture</br>
          <label class="custom-file">
              <input type="file" id="file1" name="imageFile" class="custom-file-input">
              <span class="custom-file-control"></span>
          </label>

          <br> Choose a secondary picture</br>
          <label class="custom-file">
              <input type="file" id="file2" name="imageFile2" class="custom-file-input">
              <span class="custom-file-control"></span>
          </label>
          <br></br>

          <input type="submit" value="Create Listing" class="btn btn-info btn-block "> 
      </label>
  </div>
</form>
