<section class="filter row">
    <div class="btn-group div-centered">
        <select class="custom-select">
            <option selected>Any Beds <i class="fa fa-bed" aria-hidden="true"></i></option>
            <%  var rooms = 5; %>
            <% for (var i = 0; i < rooms; i++){ %>
                <option value=""><%= i %>+ Beds</option>
            <% } %>
        </select>
    </div>
    <button type="button" class="btn btn-primary">Apply Filter</button>
</section>
    
<h1><%= title %></h1>
<p>Welcome to <%= title %></p>

<section class="row">
    <div class="results">
        <%listings.forEach(function(listing) { %>   
            <div class="thumbnail">
                <img src=<%= listing.thumbnail %> class="img-responsive" />
                <div class="caption">
                    <p>City: <%= listing.city %> </p> 
                    <p>State: <%= listing.state %> </p> 
                    <p>Number of Bedrooms: <%= listing.numBedrooms %> </p>     
                </div>
            </div>
        <% }) %>
    </div>
</section>
